/**
 *  设置RGB随机色
 */
#define colorRGB(r,g,b) [UIColor colorWithRed:(r)/255.0 green:(g)/255.0  blue:(b)/255.0  alpha:1.0]


/**
 *  通过随机数，设置RGB随机色
 */
#define randomColor colorRGB(arc4random_uniform(256), arc4random_uniform(256), arc4random_uniform(256))


